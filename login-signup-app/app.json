{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "host.quabble.client",
      "permissions": [],
      "versionCode": 14
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends as your avatar."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "name": "Quabble",
    "orientation": "portrait",

    "scheme": "quabble",
    "slug": "Quabble",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "version": "0.9.5",
    "web": {
      "favicon": "./assets/images/icon.png"
    }
  }
}
